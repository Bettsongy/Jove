// TO_St27
digraph {
	graph [rankdir=LR]
	EMPTY [label="" shape=plaintext]
	A [label=A peripheries=1 shape=circle]
	I [label=I peripheries=1 shape=circle]
	F [label=F peripheries=2 shape=circle]
	EMPTY -> I
	I -> F [label=". ; .,S"]
	I -> I [label="0 ; 0,S 
 1 ; 0,S"]
	I -> A [label="2 ; 2,L"]
	A -> F [label=". ; .,S"]
	A -> A [label="0 ; 0,R 
 . ; .,R 
 1 ; 1,R 
 2 ; 2,R"]
}
